apiVersion: v1
items:
- apiVersion: apps/v1
  kind: Deployment
  metadata:
    annotations:
      image.openshift.io/triggers: '[{"from":{"kind":"ImageStreamTag","name":"php-helloworld:latest"},"fieldPath":"spec.template.spec.containers[?(@.name==\"php-helloworld\")].image"}]'
      openshift.io/generated-by: OpenShiftNewApp
    creationTimestamp: null
    labels:
      app: php-helloworld
      app.kubernetes.io/component: php-helloworld
      app.kubernetes.io/instance: php-helloworld
    name: php-helloworld
  spec:
    replicas: 1
    selector:
      matchLabels:
        deployment: php-helloworld
    strategy: {}
    template:
      metadata:
        annotations:
          openshift.io/generated-by: OpenShiftNewApp
        creationTimestamp: null
        labels:
          deployment: php-helloworld
      spec:
        containers:
        - image: 'quay.io/redhattraining/php-hello-dockerfile'
          name: php-helloworld
          ports:
          - containerPort: 8080
            protocol: TCP
          resources: {}
  status: {}
- apiVersion: v1
  kind: Service
  metadata:
    annotations:
      openshift.io/generated-by: OpenShiftNewApp
    creationTimestamp: null
    labels:
      app: php-helloworld
      app.kubernetes.io/component: php-helloworld
      app.kubernetes.io/instance: php-helloworld
    name: php-helloworld
  spec:
    ports:
    - name: 8080-tcp
      port: 8080
      protocol: TCP
      targetPort: 8080
    selector:
      deployment: php-helloworld
  status:
    loadBalancer: {}
kind: List
metadata: {}
